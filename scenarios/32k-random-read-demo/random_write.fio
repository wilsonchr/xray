[global]
ioengine=libaio
direct=1
time_based
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson

[disk0]
bs=8k
size=2G
filename=/dev/sdb
rw=randwrite
iodepth=128

