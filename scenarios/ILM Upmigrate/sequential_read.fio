[global]
ioengine=libaio
direct=1
time_based
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson

[disk0]
bs=1m
size=2G
filename=/dev/sdb
rw=read
iodepth=4

[disk1]
bs=1m
size=2G
filename=/dev/sdc
rw=read
iodepth=4

[disk2]
bs=1m
size=2G
filename=/dev/sdd
rw=read
iodepth=4

[disk3]
bs=1m
size=2G
filename=/dev/sde
rw=read
iodepth=4

[disk4]
bs=1m
size=2G
filename=/dev/sdf
rw=read
iodepth=4

[disk5]
bs=1m
size=2G
filename=/dev/sdg
rw=read
iodepth=4
